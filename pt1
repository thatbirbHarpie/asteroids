var triangle1X = 40
var triangle1Y = 75

var triangle2X = 58
var triangle2Y = 20

var triangle3X = 80
var triangle3Y = 75

var triangleX = triangle1X, triangle2X, triangle3X
var triangleY = triangle1Y, triangle2Y, triangle3Y

function setup() {
    createCanvas(1850, 900);
  }
  
  function draw() {
    background(220);
  }

function draw() {
  background(1855)
  triangle(triangle1X, triangle1Y, triangle2X, triangle2Y, triangle3X, triangle3Y)

  move();

}

function move() {
  if(KeyIsPressed === true) {
    if(Keycode == RIGHT_ARROW) {
      triangleX +=5
    }

    if(keycode == LEFT_ARROW) {
      triangleX -=5
    }
  }
}
